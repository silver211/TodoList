<%- include('header'); -%>
    <div class="box" id="heading">
      <h1><%= ListTitle %> </h1>
    </div>
    <div class="box">
      <% for (var i=0;i<items.length;i++){%>
        <div class="item">
          <input type="checkbox">
          <p><%=items[i]%></p>
        </div>
      <%}%>

      <form  class="item" action="/" method="post">
        <input type="text" name="newitem" autocomplete="off" placeholder="What do you want to do?">
        <button type="submit" name="list" value=<%= ListTitle %>> + </button>
      </form>

    </div>

<%- include('footer'); -%>
